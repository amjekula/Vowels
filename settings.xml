<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
<servers>
  <server>
      <id>vowels-counter-app</id>
      <username>admin</username>
      <password>password</password>
    </server>
</servers>

<profiles>
  <profile>
    <id>default</id>
    <repositories>
      <repository>
        <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>vowels-counter-app</id>
      <name>vowels-counter-app</name>
      <url>http://172.17.0.2:8081/repository/vowels-counter-app/</url>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>vowels-counter-app</id>
      <name>vowels-counter-app</name>
      <url>http://172.17.0.2:8081/repository/vowels-counter-app/</url>
      </pluginRepository>
    </pluginRepositories>
  </profile>
</profiles>

<activeProfiles>
  <activeProfile>default</activeProfile>
</activeProfiles>
</settings>
